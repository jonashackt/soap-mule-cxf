<?xml version="1.0"?>
<jxb:bindings version="1.0"
			  xmlns:jxb="http://java.sun.com/xml/ns/jaxb"
			  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
			  xmlns:namespace="http://jaxb2-commons.dev.java.net/namespace-prefix"
			  xsi:schemaLocation="http://java.sun.com/xml/ns/jaxb http://java.sun.com/xml/ns/jaxb/bindingschema_2_0.xsd
			  http://jaxb2-commons.dev.java.net/namespace-prefix http://java.net/projects/jaxb2-commons/sources/svn/content/namespace-prefix/trunk/src/main/resources/prefix-namespace-schema.xsd">

	<jxb:bindings schemaLocation="Weather1.0.xsd">
		<jxb:schemaBindings>
			<jxb:package name="de.codecentric.namespace.weatherservice" />
		</jxb:schemaBindings>
		<jxb:bindings>
			<namespace:prefix name="weather" />
		</jxb:bindings>
	</jxb:bindings>
	<jxb:bindings schemaLocation="Weather-Schemes/weather-general.xsd">
		<jxb:schemaBindings>
			<jxb:package name="de.codecentric.namespace.weatherservice.general" />
		</jxb:schemaBindings>
		<jxb:bindings>
			<namespace:prefix name="general" />
		</jxb:bindings>
	</jxb:bindings>
	<jxb:bindings schemaLocation="Weather-Schemes/weather-datatypes.xsd">
		<jxb:schemaBindings>
			<jxb:package name="de.codecentric.namespace.weatherservice.datatypes" />
		</jxb:schemaBindings>
		<jxb:bindings>
			<namespace:prefix name="dtypes" />
		</jxb:bindings>
	</jxb:bindings>
	<jxb:bindings schemaLocation="Weather-Schemes/weather-exception.xsd">
		<jxb:schemaBindings>
			<jxb:package name="de.codecentric.namespace.weatherservice.exception" />
		</jxb:schemaBindings>
		<jxb:bindings>
			<namespace:prefix name="exception" />
		</jxb:bindings>
	</jxb:bindings>
</jxb:bindings>